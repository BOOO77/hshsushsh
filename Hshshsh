Tab1Section:NewToggle("AutoBlock", "Protection", function(state)

    if state then

        _G.loop = true 

        

        while _G.loop == true do 

        local args = {

    [1] = true

}

game:GetService("ReplicatedStorage").Basic.Block:FireServer(unpack(args)) 

wait()

end

    else

        _G.loop = false 

        

        while _G.loop == true do 

        local args = {

    [1] = true

}

game:GetService("ReplicatedStorage").Basic.Block:FireServer(unpack(args)) 

wait()

end

    end

end)

Tab1Section:NewButton("CopyStand","Copy ur stand",function()

local stand = 0

while true do 

    if stand >= 70 then 

       break 

    end 

    stand = stand + 1 

    wait()

    game:GetService("ReplicatedStorage"):WaitForChild("GiveStand"):FireServer()

end 

end)

